<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
"http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="alienForm" type="com.techworld.form.AlienForm" />
	</form-beans>
	<action-mappings>
		<action name="alienForm" path="/AlienInfo"
			type="com.techworld.action.AlienAction" scope="session">
			<forward name="success" path="/alien.jsp" />
		</action>

	</action-mappings>

</struts-config>